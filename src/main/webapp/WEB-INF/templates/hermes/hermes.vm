<link rel="stylesheet" href="$baseUrl/resources/css/hermes/hermes.css">
<section class="content">
    <div class="row">
        <div class="col-lg-12">
            <div class="box box-solid upload-bg-color">
                <div class="box-body phicomm-box-body">
                    <input type="file" class="input input-group" name="file" value="Choice File">
                    <input type="submit" class="input input-group phicomm-input-submit" name="submit" value="上传">
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="box box-solid">
                <div class="box-header with-border">
                    <h3 class="box-title">Hermes上传结果</h3>
                </div>
                <div class="box-body">
                    <div class="box-group" id="accordion">
                        <div class="panel box box-primary">
                            <div class="box-header with-border">
                                <h4 class="box-title">
                                    <a data-toggle="collapse" data-parent="#accordion" href="#collapseThree">
                                        基本信息
                                    </a>
                                </h4>
                            </div>
                            <div id="collapseThree" class="panel-collapse collapse in">
                                <div class="box-body">
                                    <table class="table table-bordered col-md-4">
                                        <tr>
                                            <th class="phicomm-table-index"></th>
                                            <th class="phicomm-table-context">Item</th>
                                            <th>Value</th>
                                        </tr>
                                        <tr>
                                            <td>1.</td>
                                            <td>文件名</td>
                                            <td>xxxxxxxxxxxxxxxxxxxxxxxxxxxx</td>
                                        </tr>
                                        <tr>
                                            <td>2.</td>
                                            <td>文件大小（字节）</td>
                                            <td>xxxxxxxxxxxxxxxxxxxxxxxxxxxx</td>
                                        </tr>
                                        <tr>
                                            <td>3.</td>
                                            <td>上传耗时（毫秒）</td>
                                            <td>xxxxxx</td>
                                        </tr>
                                    </table>
                                </div>
                            </div>
                        </div>
                        <div class="panel box box-primary">
                            <div class="box-header with-border">
                                <h4 class="box-title">
                                    <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne">
                                        文件哈希
                                    </a>
                                </h4>
                            </div>
                            <div id="collapseOne" class="panel-collapse collapse in">
                                <div class="box-body">
                                    <table class="table table-bordered col-md-4">
                                        <tr>
                                            <th class="phicomm-table-index"></th>
                                            <th class="phicomm-table-context">文件哈希方法</th>
                                            <th>哈希值</th>
                                        </tr>
                                        <tr>
                                            <td>1.</td>
                                            <td>MD5</td>
                                            <td>xxxxxxxxxxxxxxxxxxxxxxxxxxxx</td>
                                        </tr>
                                        <tr>
                                            <td>2.</td>
                                            <td>SHA-256</td>
                                            <td>xxxxxxxxxxxxxxxxxxxxxxxxxxxx</td>
                                        </tr>
                                    </table>
                                </div>
                            </div>
                        </div>
                        <div class="panel box box-primary">
                            <div class="box-header with-border">
                                <h4 class="box-title">
                                    <a data-toggle="collapse" data-parent="#accordion" href="#collapseTwo">
                                        下载链接
                                    </a>
                                </h4>
                            </div>
                            <div id="collapseTwo" class="panel-collapse collapse in">
                                <div class="box-body">
                                    <table class="table table-bordered col-md-4">
                                        <tr>
                                            <th class="phicomm-table-index"></th>
                                            <th class="phicomm-table-context">描述</th>
                                            <th>下载链接</th>
                                        </tr>
                                        <tr>
                                            <td>1.</td>
                                            <td>HTTPS</td>
                                            <td>xxxxxxxxxxxxxxxxxxxxxxxxxxxx</td>
                                        </tr>
                                        <tr>
                                            <td>2.</td>
                                            <td>HTTP</td>
                                            <td>xxxxxxxxxxxxxxxxxxxxxxxxxxxx</td>
                                        </tr>
                                        <tr>
                                            <td>2.</td>
                                            <td>内网使用链接</td>
                                            <td>xxxxxxxxxxxxxxxxxxxxxxxxxxxx</td>
                                        </tr>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<script src="$baseUrl/resources/js/hermes/hermes.js"></script>