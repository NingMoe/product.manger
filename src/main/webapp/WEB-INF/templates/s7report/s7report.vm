<section class="content">

#*
* 填写需要输入的信息
* *#
    <div class="phicomm_content_div">
        <div class="box box-solid phicomm_content_box" style="margin-bottom: -10px">
            <form class="form-horizontal">
                <div class="box-body">
                    <div class="row">
                        <div style=" width:100%; text-align:left; margin-left:15px; margin-right:30px;">
                            <h4 style="margin-left: 2%">合作商S7激活数量填写</h4>
                        </div>
                        <hr style="border:2 dashed #987cb9;color: #449d44; margin-top: 0px;margin-bottom: 20px"
                            width="100%"
                        />
                    </div>

                    <div class="row">
                        <div class="form-group col-md-3">
                            <label for="activationDate" class="col-sm-4 control-label">日期</label>
                            <div class="col-sm-8">
                                <input class="form-control pull-right" type="date" id="activationDate"/>
                            </div>
                        </div>
                        <div class="form-group col-md-4">
                            <label for="lianbiActivationCount" class="col-sm-5 control-label">联璧激活数量</label>
                            <div class="col-sm-7">
                                <input type="number" class="form-control pull-right" id="lianbi" value="0"
                                       oninput="checkNum('lianbi',this.value)"/>
                            </div>
                        </div>
                        <div class="form-group col-md-4">
                            <label for="wanjiaActivationCount" class="col-sm-5 control-label">万家金服激活数量</label>
                            <div class="col-sm-7">
                                <input type="number" class="form-control pull-right" id="wanjia" value="0"
                                       oninput="checkNum('wanjia',this.value)"/>
                            </div>
                        </div>
                        <div class="form-group col-md-1" style="margin-left: 50px">
                            <div>
                                <button type="button" class="btn btn-primary pull-right phicomm_button_go"
                                        onclick="addedKKeys()">
                                    提交
                                </button>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div style=" width:100%; text-align:left; margin-left:15px; margin-right:30px;margin-top: 20px; ">
                            <h4 style="margin-left: 2%">不同设备活跃用户量填写</h4>
                        </div>
                        <hr style="border:1 dashed #987cb9; margin-top: 0px;margin-bottom: 20px" width="100%"
                            color=#987cb9 SIZE=1/>
                    </div>
                    <div class="row">
                        <div class="form-group col-md-3">
                            <label for="activeuserDate" class="col-sm-4 control-label">日期</label>
                            <div class="col-sm-8">
                                <input class="form-control pull-right" type="date" id="activeuserDate"/>
                            </div>
                        </div>
                        <div class="form-group col-md-4">
                            <label for="IOSActiveUserCount" class="col-sm-5 control-label">IOS活跃用户</label>
                            <div class="col-sm-7">
                                <input type="number" class="form-control pull-right" id="iosActive" value="0"
                                       oninput="checkNum('iosActive',this.value)"/>
                            </div>
                        </div>
                        <div class="form-group col-md-4">
                            <label for="AndroidActiveUserCount" class="col-sm-5 control-label">Android活跃用户</label>
                            <div class="col-sm-7">
                                <input type="number" class="form-control pull-right" id="androidActive" value="0"
                                       oninput="checkNum('androidActive',this.value)"/>
                            </div>
                        </div>
                        <div class="form-group col-md-1" style="margin-left: 50px">
                            <div>
                                <button type="button" class="btn btn-primary pull-right phicomm_button_go"
                                        onclick="getAll()">
                                    提交
                                </button>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div style=" width:100%; text-align:left; margin-left:15px; margin-right:30px;">
                            <h4 style="margin-left: 2%">获取报告</h4>
                        </div>
                        <hr style="border:1 dashed #987cb9; margin-top: 0px;margin-bottom: 20px" width="100%"
                            color=#987cb9 SIZE=1/>
                    </div>

                    <div class="row">
                        <div class="form-group col-md-3">
                            <label for="reportDate" class="col-sm-4 control-label">日期</label>
                            <div class="col-sm-8">
                                <input class="form-control pull-right" type="date" id="reportDate"/>
                            </div>
                        </div>
                        <div class="form-group col-md-1" style="margin-left: 50px">
                            <div>
                                <button type="button" class="btn btn-primary pull-right phicomm_button_go"
                                        onclick="getReport()">
                                    获取
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
        </div>


    ##      报告总结部分
